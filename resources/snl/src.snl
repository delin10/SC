{域变量的测试程序}
{功能：对于输入m,n ;  输出m+1,n+1,和m+1+n+1的值}
program sd
type  i=integer;
      sarray=array [0..5] of integer;
var  i x;
     sarray y;
     integer ss;
procedure sd(integer a);
begin
   write(a)
end
begin
  ss:=0;
  while ss<6 do
    y[ss]:=ss;
    x:=y[ss];
    sd(x);
    ss:=ss+1
  endwh
end.
